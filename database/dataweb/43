<link href="https://news.files.bbci.co.uk/include/newsspec/36019-coronation-crowns-threejs/assets/full-width/css/app.css??v=1.0.0.202305051243" rel="stylesheet" type="text/css" media="all" />
<script>
function cutsTheMustard() {
    // var hasWebp = document.createElement('canvas').toDataURL('image/webp').indexOf('data:image/webp') > -1;
    var hasWebp = true;

    var BrowserDetect = {
        init: function () {
            this.browser = this.searchString(this.dataBrowser) || "An unknown browser";
            this.version = this.searchVersion(navigator.userAgent)
                || this.searchVersion(navigator.appVersion)
                || "an unknown version";
            this.OS = this.searchString(this.dataOS) || "an unknown OS";
            //
            // Safari and Firefox webp check here
            //
            if (this.OS === 'iPhone/iPod' && this.browser === 'Safari' && this.version <= 14) hasWebp = false;
            if (this.OS !== 'iPhone/iPod' && this.browser === 'Safari' && this.version < 13.7) hasWebp = false;
            if (this.browser === 'Firefox' && this.version < 65) hasWebp = false;

            // check for Chrome browser on iOS <= 14
            const ChromeOnIOS = navigator.userAgent.indexOf('CriOS') >= 0;
            const checkVersion = () => /(iPhone|iPad) OS ([1-9]*)/g.exec(window.navigator.userAgent)?.[2] || 0
            const iosVersion = checkVersion();
            if (ChromeOnIOS && iosVersion <= 14) {
                hasWebp = false;
            }

            // console.log({
            //     OS: this.OS,
            //     browser: this.browser,
            //     version: this.version,
            //     iosVersion: iosVersion,
            //     ChromeOnIOS: ChromeOnIOS,
            //     hasWebp: hasWebp,
            // });
            // var elem = document.createElement('div');
            // elem.innerHTML = this.browser + ' ' + this.version + ' (' + this.OS + ')';
            // document.body.appendChild(elem);
        },
        searchString: function (data) {
            for (var i=0;i<data.length;i++) {
                var dataString = data[i].string;
                var dataProp = data[i].prop;
                this.versionSearchString = data[i].versionSearch || data[i].identity;
                if (dataString) {
                    if (dataString.indexOf(data[i].subString) != -1)
                        return data[i].identity;
                }
                else if (dataProp)
                    return data[i].identity;
            }
        },
        searchVersion: function (dataString) {
            var index = dataString.indexOf(this.versionSearchString);
            if (index == -1) return;
            return parseFloat(dataString.substring(index+this.versionSearchString.length+1));
        },
        dataBrowser: [
            {
                string: navigator.userAgent,
                subString: "Chrome",
                identity: "Chrome"
            },
            {
                string: navigator.vendor,
                subString: "Apple",
                identity: "Safari",
                versionSearch: "Version"
            },
            {
                string: navigator.vendor,
                subString: "AppleWebKit",
                identity: "Safari",
                versionSearch: "Version"
            },
            {
                string: navigator.vendor,
                subString: "CriOS",
                identity: "Chrome",
                versionSearch: "Version"
            },
            {
                string: navigator.vendor,
                subString: "AppleWebKit",
                identity: "AppleWebKit",
                versionSearch: "Version"
            },
            {
                string: navigator.userAgent,
                subString: "Firefox",
                identity: "Firefox"
            },
        ],
        dataOS : [
            {
                string: navigator.platform,
                subString: "Win",
                identity: "Windows"
            },
            {
                string: navigator.platform,
                subString: "Mac",
                identity: "Mac"
            },
            {
                   string: navigator.userAgent,
                   subString: "iPhone",
                   identity: "iPhone/iPod"
            },
            {
                string: navigator.platform,
                subString: "Linux",
                identity: "Linux"
            }
        ]

    };
    BrowserDetect.init();

    return (
        document.implementation.hasFeature('http://www.w3.org/TR/SVG11/feature#BasicStructure', '1.1') &&
        'querySelector' in document &&
        'localStorage' in window &&
        'addEventListener' in window &&
        'MutationObserver' in window && // not supported in IE9 & IE10
        !document.documentMode && // undefined in all browsers except IE
        hasWebp === true
    );
}

                    if (cutsTheMustard()) {
                                document.getElementById('loading-spinner').className = 'bbc-news-visual-journalism-loading-spinner';
        var suffix = ('noModule' in HTMLScriptElement.prototype) ? '-es6' : '';

        var requireConfig = {
                urlArgs: '?v=1.0.0.202305051243',
                baseUrl: 'https://news.files.bbci.co.uk/include/newsspec/36019-coronation-crowns-threejs/assets/full-width/js/',
            },
            moduleToStartApp = 'https://news.files.bbci.co.uk/include/newsspec/36019-coronation-crowns-threejs/portuguese/app' + suffix + '.js',
            wrapper = 'https://news.files.bbci.co.uk/include/newsspec/36019-coronation-crowns-threejs/assets/full-width/js/wrapper';
            requireConfig.paths = {
                'news-vj-ati-event-tracking': 'https://news.files.bbci.co.uk/include/newsspec/js/vendor/news-vj-ati-event-tracking-2.0.20',
            };

        require.config(requireConfig);
        require(['wrapper'], function (wrapper) {
            wrapper.initwrapper(window.vjConfigObject['newsspec-36019-coronation-crowns-threejs-app']);
            require([moduleToStartApp]);
        });

                    }
                    else if (window.require) {
                        require(['istats-1'], function (istats) {
                            istats.log('browser does not cut the mustard', 'newsspec-nonuser');
                        });
                    }    var chartbeatAttempts = 0;
</script>
