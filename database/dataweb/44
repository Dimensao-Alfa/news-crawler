            wrapper = 'https://news.files.bbci.co.uk/include/newsspec/36019-coronation-crowns-threejs/assets/full-width/js/wrapper';
            requireConfig.paths = {
                'news-vj-ati-event-tracking': 'https://news.files.bbci.co.uk/include/newsspec/js/vendor/news-vj-ati-event-tracking-2.0.20',
            };

