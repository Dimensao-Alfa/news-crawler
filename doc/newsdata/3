const EXPERIMENT_NAME = 'g1-mc-recirculation-top-v1';
;// CONCATENATED MODULE: ./show/recirculacao-topo-mc/src/javascripts/modules/AB.js

class AB {
  constructor(experimentName, controlProperties) {
    this.experimentName = experimentName;
    this.controlProperties = controlProperties;
    return (async () => {
      await globalThis.globalWebdeps('globo-ab');
      this.abClient = new window.GloboAB(ENV);
      this.abClient.options().timeout(3000);
      return this;
    })();
  }
  async chooseExperiment() {
    let experiment;
    try {
      experiment = await this.abClient.choose(this.experimentName);
    } catch (error) {
      experiment = this.getControlAlternative();
      console.error(this.experimentName, error);
    }
    return experiment;
  }
  getControlAlternative() {
    return {
      alternative: 'control',
      experiment: this.experimentName,
      properties: {
        ...this.controlProperties
      }
    };
  }
}
/* harmony default export */ const modules_AB = (AB);
;// CONCATENATED MODULE: ./show/recirculacao-topo-mc/src/javascripts/web.critical.js
/* web(desktop) critical javascript */





(async function main() {
  try {
    const recTopoMc = document.getElementById('rec-topo-mc');
    if (recTopoMc) {
      initBindEvents();
      const recTopoAB = await new modules_AB(EXPERIMENT_NAME);
      const recTopoExperiment = await recTopoAB.chooseExperiment();
      switch (recTopoExperiment.alternative) {
        case 'versao-a':
          recTopoMc.style.display = '';
          break;
        case 'controle':
        default:
          break;
      }
    } else {
      console.error('[recirculacao-topo-mc] element rec-topo-mc is missing');
    }
  } catch (err) {
    console.error('[recirculacao-topo-mc]', err);
  }
})();
})();

