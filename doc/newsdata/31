                    	localStorage.setItem(storageKey, JSON.stringify(forStorage));
                    	if (shouldAttachStyle) {
                    		const styleInnerText = '@font-face{font-family: "' + font.fontFamily + '"; font-weight: ' + font.fontWeight + ';src:url("' + fontContents + '") format("woff2");font-display: swap;}';
                    		createStyleAndAttach(styleInnerText);
                		}
                    });
                    });
                };
                fontsForStorage.forEach(font => {
                    const storageKey = 'font-' + font.name;
                    let fontContents = localStorage.getItem(storageKey);

                    if (!fontContents) {
                        retrieveAndStoreFont(font, storageKey, true);
                    }
                    else {
                    	const { base64Contents, fontFamily, fontWeight, fontVersion } = JSON.parse(fontContents);
                    	const styleInnerText = '@font-face{font-family: "' + fontFamily + '"; font-weight: ' + fontWeight + '; src:url("' + base64Contents + '") format("woff2");font-display: swap;}';
                		createStyleAndAttach(styleInnerText);
                		if (fontVersion !== font.version) {
                			retrieveAndStoreFont(font, storageKey, false);
                		}
                    }
                });
                }
    </script><script data-react-helmet="true" async="" type="text/javascript" src="https://static.files.bbci.co.uk/ws/simorgh-assets/public/static/js/comscore/main-1.0.js"></script><script data-react-helmet="true" type="application/ld+json">{"@context":"http://schema.org","@graph":[{"@type":"ReportageNewsArticle","url":"https://www.bbc.com/portuguese/articles/c4nj1e42093o","publisher":{"@type":"NewsMediaOrganization","name":"BBC News Brasil","publishingPrinciples":"https://www.bbc.com/portuguese/institutional-50054434","logo":{"@type":"ImageObject","width":1024,"height":576,"url":"https://news.files.bbci.co.uk/include/articles/public/portuguese/images/metadata/poster-1024x576.png"}},"thumbnailUrl":"https://ichef.bbci.co.uk/news/1024/branded_portuguese/7b80/live/8560a770-fb21-11ed-92cc-b3a9bf1f67e9.jpg","image":{"@type":"ImageObject","width":1024,"height":576,"url":"https://ichef.bbci.co.uk/news/1024/branded_portuguese/7b80/live/8560a770-fb21-11ed-92cc-b3a9bf1f67e9.jpg"},"mainEntityOfPage":{"@type":"WebPage","@id":"https://www.bbc.com/portuguese/articles/c4nj1e42093o","name":"Como funcionará programa do governo que pode baratear carros no Brasil"},"headline":"Como funcionará programa do governo que pode baratear carros no Brasil","datePublished":"2023-05-25T23:44:09.521Z","dateModified":"2023-05-25T23:44:09.521Z","inLanguage":{"@type":"Language","name":"Portuguese","alternateName":"pt-BR"},"about":[{"@type":"Thing","name":"Política","sameAs":["http://dbpedia.org/resource/Politics"]},{"@type":"Thing","name":"Economia","sameAs":["http://dbpedia.org/resource/Economy"]}],"author":{"@type":"Person","name":"Giulia Granchi"}}]}</script><script data-react-helmet="true" type="text/javascript">
