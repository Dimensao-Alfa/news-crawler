                    	localStorage.setItem(storageKey, JSON.stringify(forStorage));
                    	if (shouldAttachStyle) {
                    		const styleInnerText = '@font-face{font-family: "' + font.fontFamily + '"; font-weight: ' + font.fontWeight + ';src:url("' + fontContents + '") format("woff2");font-display: swap;}';
                    		createStyleAndAttach(styleInnerText);
                		}
                    });
                    });
                };
                fontsForStorage.forEach(font => {
                    const storageKey = 'font-' + font.name;
                    let fontContents = localStorage.getItem(storageKey);

                    if (!fontContents) {
                        retrieveAndStoreFont(font, storageKey, true);
                    }
                    else {
                    	const { base64Contents, fontFamily, fontWeight, fontVersion } = JSON.parse(fontContents);
                    	const styleInnerText = '@font-face{font-family: "' + fontFamily + '"; font-weight: ' + fontWeight + '; src:url("' + base64Contents + '") format("woff2");font-display: swap;}';
                		createStyleAndAttach(styleInnerText);
                		if (fontVersion !== font.version) {
                			retrieveAndStoreFont(font, storageKey, false);
                		}
                    }
                });
                }
    </script><script data-react-helmet="true" async="" type="text/javascript" src="https://static.files.bbci.co.uk/ws/simorgh-assets/public/static/js/comscore/main-1.0.js"></script><script data-react-helmet="true" type="application/ld+json">{"@context":"http://schema.org","@graph":[{"@type":"ReportageNewsArticle","url":"https://www.bbc.com/portuguese/articles/c4n7q7e7r3ro","publisher":{"@type":"NewsMediaOrganization","name":"BBC News Brasil","publishingPrinciples":"https://www.bbc.com/portuguese/institutional-50054434","logo":{"@type":"ImageObject","width":1024,"height":576,"url":"https://news.files.bbci.co.uk/include/articles/public/portuguese/images/metadata/poster-1024x576.png"}},"thumbnailUrl":"https://ichef.bbci.co.uk/news/1024/branded_portuguese/3c3c/live/8c9a7d70-f99d-11ed-92cc-b3a9bf1f67e9.jpg","image":{"@type":"ImageObject","width":1024,"height":576,"url":"https://ichef.bbci.co.uk/news/1024/branded_portuguese/3c3c/live/8c9a7d70-f99d-11ed-92cc-b3a9bf1f67e9.jpg"},"mainEntityOfPage":{"@type":"WebPage","@id":"https://www.bbc.com/portuguese/articles/c4n7q7e7r3ro","name":"A 'fórmula do sucesso' usada por recrutadores do Google"},"headline":"A 'fórmula do sucesso' usada por recrutadores do Google","datePublished":"2023-05-24T18:37:34.651Z","dateModified":"2023-05-24T18:37:34.651Z","inLanguage":{"@type":"Language","name":"Portuguese","alternateName":"pt-BR"},"about":[{"@type":"Thing","name":"Comportamento"},{"@type":"Thing","name":"Desemprego","sameAs":["http://dbpedia.org/resource/Unemployment"]},{"@type":"Thing","name":"Negócios","sameAs":["http://dbpedia.org/resource/Business"]},{"@type":"Thing","name":"Empreendedorismo","sameAs":["http://dbpedia.org/resource/Entrepreneurship"]},{"@type":"Thing","name":"Google","sameAs":["http://dbpedia.org/resource/Google"]},{"@type":"Thing","name":"Sociedade","sameAs":["http://dbpedia.org/resource/Society"]},{"@type":"Thing","name":"Emprego","sameAs":["http://dbpedia.org/resource/Employment"]}],"author":{"@type":"Person","name