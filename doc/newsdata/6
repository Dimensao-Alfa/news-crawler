;// CONCATENATED MODULE: ./show/recirculacao-topo-mc/src/javascripts/modules/horizon-client.js
class HorizonClient {
  constructor() {
    this.client = null;
  }
  setHorizonClient() {
    setTimeout(() => {
      if (window.HorizonClient) {
        return window.HorizonClient.then(client => this.client = client);
      } else {
        this.setHorizonClient();
      }
    }, 100);
  }
  send(options) {
    this.client && this.client.send(Object.assign(options));
  }
  commonEvent(_ref) {
    let {
      eventCategory,
      eventAction,
      eventLabel,
      eventValue,
      eventProperty
    } = _ref;
    this.send({
      id: 'common-event',
      version: '0.1',
      contentType: 'common',
      properties: {
        eventCategory,
        eventAction,
        eventLabel,
        eventValue,
        eventProperty
      }
    });
  }
}
/* harmony default export */ const horizon_client = (HorizonClient);
;// CONCATENATED MODULE: ./show/recirculacao-topo-mc/src/javascripts/modules/bind-events.js

const initBindEvents = () => {
  try {
    const recTopoMc = document.getElementById('rec-topo-mc');
    const btnR = document.getElementById("rec-topo-mc__btnRight");
    const horizonClient = new horizon_client();
    horizonClient.setHorizonClient();
    let alreadyScrolled = false;
    function trackScrollEvent() {
      if (!alreadyScrolled) {
        horizonClient.commonEvent({
          eventCategory: `recirculation top`,
          eventAction: `scroll`,
          eventLabel: '',
          eventValue: null,
          eventProperty: null
        });
        alreadyScrolled = true;
      }
    }
    function bindEvents() {
      recTopoMc.addEventListener('scroll', trackScrollEvent);
      btnR.addEventListener("click", trackScrollEvent);
      recTopoMc.querySelectorAll('a').forEach(el => {
        el.addEventListener('click', function (e) {
          e.preventDefault();
          horizonClient.commonEvent({
            eventCategory: `recirculation top`,
            eventAction: `click`,
            eventLabel: `${el.innerHTML.trim()} | ${el.dataset.index}`,
            eventValue: null,
            eventProperty: `${el.href}`
          });
          window.location.href = el.dataset.url;
        });
      });
      setTimeout(() => {
        horizonClient.commonEvent({
          eventCategory: 'recirculation top',
          eventAction: 'impression',
          eventLabel: '',
          eventValue: null,
          eventProperty: null
        });
      }, 300);
    }
    bindEvents();
  } catch (err) {
    console.error('[recirculacao-topo-mc]', err);
  }
};

;// CONCATENATED MODULE: ./show/recirculacao-topo-mc/src/javascripts/modules/constants.js
const ENV = window.glb && window.glb.getCurrentEnv && window.glb.getCurrentEnv() || 'qa';
const EXPERIMENT_NAME = 'g1-mc-recirculation-top-v1';
;// CONCATENATED MODULE: ./show/recirculacao-topo-mc/src/javascripts/modules/AB.js

class AB {
  constructor(experimentName, controlProperties) {
    this.experimentName = experimentName;
    this.controlProperties = controlProperties;
    return (async () => {
      await globalThis.globalWebdeps('globo-ab');
      this.abClient = new window.GloboAB(ENV);
      this.abClient.options().timeout(3000);
      return this;
    })();
  }
  async chooseExperiment() {
    let experiment;
    try {
      experiment = await this.abClient.choose(this.experimentName);
    } catch (error) {
      experiment = this.getControlAlternative();
      console.error(this.experimentName, error);
    }
    return experiment;
  }
  getControlAlternative() {
    return {
      alternative: 'control',
      experiment: this.experimentName,
      properties: {
        ...this.controlProperties
      }
    };
  }
}
/* harmony default export */ const modules_AB = (AB);
;// CONCATENATED MODULE: ./show/recirculacao-topo-mc/src/javascripts/web.critical.js
/* web(desktop) critical javascript */





(async function main() {
  try {
    const recTopoMc = document.getElementById('rec-topo-mc');
    if (recTopoMc) {
      initBindEvents();
      const recTopoAB = await new modules_AB(EXPERIMENT_NAME);
      const recTopoExperiment = await recTopoAB.chooseExperiment();
      switch (recTopoExperiment.alternative) {
        case 'versao-a':
          recTopoMc.style.display = '';
          break;
        case 'controle':
        default:
          break;
      }
    } else {
      console.error('[recirculacao-topo-mc] element rec-topo-mc is missing');
    }
  } catch (err) {
    console.error('[recirculacao-topo-mc]', err);
  }
})();
})();

/******/ })()
